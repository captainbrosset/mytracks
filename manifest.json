{
    "lang": "en-us",
    "name": "My Tracks",
    "short_name": "mytracks",
    "description": "Import, export and visualize GPS tracks",
    "start_url": "/",
    "background_color": "#000000",
    "theme_color": "#000000",
    "orientation": "any",
    "display": "standalone",
    "icons": [
        {
            "src": "media/icon512.png",
            "sizes": "512x512"
        }
    ],
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "*/*": [
                    ".gpx"
                ]
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "geo",
            "url": "/?show_place=%s"
        }
    ],
    "display_override": ["window-controls-overlay"]
}
